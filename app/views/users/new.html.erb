<% provide(:title, 'Sign up') %>
<h1>Sign up</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= render 'shared/error_messages', user: @user %>
    <%= form_for @user, url: signup_path do |f| %>
      <%= render partial: 'form', locals: { form_builder: f } %>
    <% end %>
  </div>
</div>
